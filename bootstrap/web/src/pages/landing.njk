---
pagination: { data: pages, size: 1, alias: page }
permalink: "/{{ page.slug | url }}/index.html"
layout: base.njk
eleventyComputed: { title: "{{ page.title }}" }
---
{% if globals and globals.header %}
  {% set block = globals.header %}
  {% include "blocks/header.njk" %}
{% endif %}

<h1>{{ page.title }}</h1>
{% for block in page.blocks %}
  {% if block._type != 'header' %}
    {% include "blocks/" + block._type + ".njk" %}
  {% endif %}
{% endfor %}
